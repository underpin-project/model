    schema:value "10"^^xsd:float ;
    schema:unitCode "KMT"
  ] ;
  dct:hasPart [
    a dcat:Dataset ;
    dct:title "Wind Speed Data" ;
    dct:description "Subset of the dataset focusing on wind speed measurements."
  ] .
